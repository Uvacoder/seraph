{
  "name": "seraph",
  "version": "0.1.0",
  "private": true,
  "author": "Quavo",
  "scripts": {
    "dev": "next dev",
    "build": "NODE_ENV=production next build",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "vercel-build": "prisma generate --schema=./src/lib/prisma/schema.prisma && prisma migrate deploy --schema=./src/lib/prisma/schema.prisma && next build",
    "prisma:generate": "prisma generate",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && yarn format",
    "type-check": "tsc --noEmit",
    "format": "prettier --write src",
    "up": "yarn upgrade-interactive",
    "up-latest": "yarn up --latest",
    "release": "standard-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky install",
    "test:unit": "jest",
    "test:e2e:open": "cypress open",
    "test:e2e:run": "cypress run"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.8.6",
    "@codesandbox/sandpack-react": "^0.18.0",
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@monaco-editor/react": "^4.4.1",
    "@next-auth/prisma-adapter": "^1.0.1",
    "@prisma/client": "^3.10.0",
    "@uiw/react-monacoeditor": "^3.5.3",
    "file-saver": "^2.0.5",
    "framer-motion": "^5.6.0",
    "monaco-editor": "^0.33.0",
    "monaco-themes": "^0.4.1",
    "next": "^12.1.5",
    "next-auth": "^4.9.0",
    "next-pwa": "^5.4.5",
    "next-seo": "^5.1.0",
    "next-transpile-modules": "^9.0.0",
    "nprogress": "^0.2.0",
    "prisma": "^3.10.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-dropzone": "^12.0.4",
    "react-icons": "^4.3.1",
    "react-share": "^4.4.0",
    "react-syntax-highlighter": "^15.5.0",
    "sharp": "^0.30.5"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^16.2.1",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.2",
    "@types/file-saver": "^2.0.5",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^18.0.6",
    "@types/react-dom": "^18.0.2",
    "@types/react-syntax-highlighter": "^13.5.2",
    "commitlint": "^16.2.1",
    "cypress": "^9.5.4",
    "cypress-social-logins": "^1.14.0",
    "eslint": "^8.9.0",
    "eslint-config-next": "^12.1.0",
    "eslint-config-sznm": "^0.2.1",
    "eslint-plugin-cypress": "^2.12.1",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.5",
    "mutationobserver-shim": "^0.3.7",
    "next-sitemap": "^2.5.1",
    "prettier": "^2.5.1",
    "standard-version": "^9.3.2",
    "typescript": "^4.6.2"
  }
}
